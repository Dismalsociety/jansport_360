print(f"Actual pressure range: {targets_actual.min():.2f} to {targets_actual.max():.2f}")
print(f"Predicted pressure range: {predictions_actual.min():.2f} to {predictions_actual.max():.2f}")
print(f"Number of predictions outside [0, 3000]: {np.sum((predictions_actual < 0) | (predictions_actual > 3000))}")
print(f"Worst prediction error: {np.max(np.abs(targets_actual - predictions_actual)):.2f}")
