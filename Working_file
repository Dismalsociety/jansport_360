# SIMPLEST POSSIBLE TEST
from sklearn.linear_model import LinearRegression
from sklearn.metrics import r2_score

# Load data
df = pd.read_excel('')
flows = df['Flow Rate'].values
pressures = df['Pressure'].values

# Simple: current flow → current pressure (no sequences!)
lr = LinearRegression()
lr.fit(flows.reshape(-1, 1), pressures)
pred = lr.predict(flows.reshape(-1, 1))
r2 = r2_score(pressures, pred)

print(f"Simple flow→pressure R²: {r2:.4f}")
